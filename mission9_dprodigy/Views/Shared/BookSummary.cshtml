
@{
}

@model BookModel

<div class="card">
    <div class="card-body">
        <h4>@Model.Title</h4>
        <ul>
            <li>Author: @Model.Author</li>
            <li>Publisher: @Model.Publisher</li>
            <li>ISBN: @Model.Isbn</li>
            <li>Category: @Model.Category</li>
            <li>Number of Pages: @Model.PageCount</li>
            <li>Price: @Model.Price</li>
        </ul>
    </div>
</div>